# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug_x32
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug_x32)
  xerceslua_config = debug_x32
endif
ifeq ($(config),debug_x64)
  xerceslua_config = debug_x64
endif
ifeq ($(config),debug_native)
  xerceslua_config = debug_native
endif
ifeq ($(config),release_x32)
  xerceslua_config = release_x32
endif
ifeq ($(config),release_x64)
  xerceslua_config = release_x64
endif
ifeq ($(config),release_native)
  xerceslua_config = release_native
endif

PROJECTS := xerceslua

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

xerceslua:
ifneq (,$(xerceslua_config))
	@echo "==== Building xerceslua ($(xerceslua_config)) ===="
	@${MAKE} --no-print-directory -C . -f xerceslua.make config=$(xerceslua_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f xerceslua.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug_x32"
	@echo "  debug_x64"
	@echo "  debug_native"
	@echo "  release_x32"
	@echo "  release_x64"
	@echo "  release_native"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   xerceslua"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"